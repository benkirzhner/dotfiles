[user]
    name =  <%= puts "Name: "; STDOUT.flush; STDIN.gets.chomp %>
    email = <%= puts "Email: "; STDOUT.flush; STDIN.gets.chomp %>
[core]
    editor = vim
[alias]
    graph = log --all --decorate --graph
[apply]
    whitespace = strip
[diff]
    color = auto
    rename = copy
[pager]
    color = true
[status]
    color = auto
[github]
    user =  <%= puts "Github Username: "; STDOUT.flush; STDIN.gets.chomp %>
    token = <%= puts "Github API Token: "; STDOUT.flush; STDIN.gets.chomp %>
